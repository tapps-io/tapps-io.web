{
  "schema": "http://127.0.0.1:1337/graphql",
  "documents": "./**/*.gql",
  "extensions": {
    "codegen": {
      "generates": {
        "./generated/graphql.tsx": {
          "config": {
            "withHooks": true
          },
          "plugins": [
            {
              "add": "/* eslint-disable */"
            },
            "typescript",
            "typescript-operations",
            "typescript-react-apollo"
          ]
        }
      }
    }
  }
}
